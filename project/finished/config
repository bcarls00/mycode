en
conf t
hostname switch1
username admin password passpass
enable secret passpass
interface vlan #VLAN Number
ip address #VLAN IP #VLAN SM
exit
interface vlan 99
ip address 10.0.0.1 255.255.255.0
exit
interface vlan 100
ip address 10.0.0.2 255.255.255.0
exit
interface vlan 56
ip address 10.0.0.3 255.255.255.0
exit
interface vlan 35
ip address 10.0.0.4 255.255.255.0
exit
interface vlan 48
ip address 10.0.0.5 255.255.255.0
exit
interface vlan 990
ip address 10.0.0.6 255.255.255.0
exit
interface vlan #Status
ip address #VLAN on/off #VLANs
exit
interface #Fast Eth Ports
shutdown
exit
interface fa0/1
no shutdown
swithcport mode access
switchport access vlan "99
exit
interface fa0/2
no shutdown
swithcport mode access
switchport access vlan "99
exit
interface fa0/3
no shutdown
swithcport mode access
switchport access vlan "99
