username admin password pass
enable secret passpass
interface vlan 99
ip address 10.0.0.1 
exit
interface vlan 100
ip address 10.0.0.2 255.255.255.0

exit
interface vlan 56
ip address 10.0.0.3 255.255.255.0

exit
